{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api.py"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api.py"
    }
  ]
}

